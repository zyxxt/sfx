{
    "presets": [
        [
            "es2015",
            {
                "loose": true
            }
        ],
        "stage-0"
    ],
    "plugins": [
        "transform-runtime",
        "transform-vue-jsx",
        [
            "transform-es2015-modules-commonjs",
            {
                "strict": false,
                "loose": true
            }
        ],
        [
            "transform-strict-mode",
            {
                "strict": false
            }
        ]
    ],
    "comments": false
}
